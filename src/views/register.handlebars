<div id="contenedor">
        <form id="registerForm">
            <h2>Registro</h2>
            <input class="controls" type="text" name="first_name" placeholder="Ingrese Nombre" required>
            <input class="controls" type="text" name="last_name" placeholder="Ingrese Apellido" required>
            <input class="controls" type="email" name="email" placeholder="Ingrese su Email" required>
            <input class="controls" type="text" name="username" placeholder="Ingrese username" required>
            <input class="controls" type="text" name="age" placeholder="Ingrese su Edad" required>
            <input class="controls" type="password" name="password" placeholder="Ingrese su contraseña" required>
            <button class="botons" type="submit" >Registrar </button>
        <p>¿Ya tienes una cuenta? <a href="/login">Ingrese aquí</a></p>
        <button onclick="location.href='/api/auth/githubSignup'">Ingresar con Github</button>
        </form>
</div>
<script src="/js/register.js"></script>